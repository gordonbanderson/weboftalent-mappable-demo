{
	"name": "weboftalent/mappabledemo",
	"description": "Demo for mappable module",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "google maps", "maps", "mappable", "demo"],
	"authors": [
		{
			"name": "Gordon Anderson",
			"email": "gordon.b.anderson@gmail.com",
			"homepage": "https://github.com/gordonbanderson/weboftalent-mappable-demo",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"silverstripe/framework": "^4@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"WebOfTalent\\MappableDemo\\": "src"
		}
	},

	"extra": {
		"installer-name": "mappable-demo",
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		},
		"expose": [
			"data"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,

	"suggest": {
		"weboftalent/mappable-poi": "1.*"
	},
    "support": {
        "issues": "https://github.com/gordonbanderson/weboftalent-mappable-demo/issues"
    },
	"license": "BSD-3-Clause"
}
